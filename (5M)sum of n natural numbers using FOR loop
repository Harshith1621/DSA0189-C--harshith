#include <iostream>
int main() {
    int n, sum = 0, number;
    std::cout << "Enter the value of n: ";
    std::cin >> n;
    std::cout << "Enter the numbers: ";
    for (int i = 0; i < n; ++i) {
        std::cin >> number; 
        sum += number;
    }
    std::cout << "The sum of the entered numbers is: " << sum << std::endl;
    return 0;
}
